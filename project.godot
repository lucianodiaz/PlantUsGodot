; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PlantUs"
run/main_scene="res://Scenes/global_scene.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

PlayerInfoScript="res://Scripts/playerInfoScript.gd"
SeedsType="*res://Scripts/seeds_type.gd"
Events="*res://Scripts/global/Events.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://Resources/Textures/UI/Cursor/White-Grey-Cat-Paw.png"

[editor]

naming/node_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/codeandweb.texturepacker/plugin.cfg")

[filesystem]

import/blender/enabled=false

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
